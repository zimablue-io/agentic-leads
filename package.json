{
	"name": "@agentic-leads/root",
	"private": true,
	"scripts": {
		"build": "turbo run build",
		"dev": "turbo run dev",
		"lint": "turbo run lint",
		"format": "prettier --write \"**/*.{ts,tsx,md}\"",
		"check-types": "turbo run check-types",
		"db:migrate": "pnpm --filter @agentic-leads/supabase run db:push",
		"db:pull": "pnpm --filter @agentic-leads/supabase run db:pull",
		"db:seed": "pnpm --filter @agentic-leads/supabase run db:seed"
	},
	"devDependencies": {
		"prettier": "^3.6.0",
		"ts-node": "^10.9.2",
		"tsx": "^4.7.0",
		"turbo": "^2.5.4",
		"typescript": "5.8.2"
	},
	"packageManager": "pnpm@9.0.0",
	"engines": {
		"node": ">=18"
	}
}
